---
import Layout from "@layouts/Layout.astro"
import InnerHero from "@components/sections/InnerHero.astro"
import ContactInfo from "@components/sections/ContactInfo.astro"
import developerImg from '@assets/images/home/developer.jpg';


const heroContent = {
    title: "Contacto",
    description: "Contactá con nosotros para más información",
    backgroundImage: developerImg,
    overlayOpacity: 0.5
    
}
---

<Layout title="Contacto" description="Contact us for more information">
    <InnerHero content={heroContent} />
    <ContactInfo background="base" padding="base" />

</Layout>